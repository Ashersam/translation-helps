  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "babel-preset-react-app": "^7.0.1",
    "eslint-config-react-app": "^5.0.1",
    "eslint-plugin-react-hooks": "^1.0.1",
    "gh-pages": "^2.0.1",
    "rimraf": "^2.6.3"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@0.0.15"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
