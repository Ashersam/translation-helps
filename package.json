    "axios": "^0.18.0",
    "axios-cache-adapter": "^2.2.1",
    "deep-freeze": "0.0.1",
    "js-yaml-parser": "^1.0.0",
    "jszip": "^3.2.0",
    "local-storage": "^1.4.2",
    "localforage": "^1.7.3",
    "mui-datatables": "2.0.0-beta.56",
